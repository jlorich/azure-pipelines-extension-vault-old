    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "VaultInstaller",
            "envFile": "${workspaceFolder}/Tasks/VaultInstaller/.env",
            "program": "${workspaceFolder}/Tasks/VaultInstaller/src/Index.ts",
            "preLaunchTask": "Compile VaultInstaller",
            "internalConsoleOptions": "openOnSessionStart",
            "sourceMaps": true,
            "outputCapture": "std",
            "outFiles": [
                "${workspaceFolder}/Tasks/VaultInstaller/.bin/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Vault",
            "envFile": "${workspaceFolder}/Tasks/Vault/.env",
            "program": "${workspaceFolder}/Tasks/Vault/src/Index.ts",
            "preLaunchTask": "Compile Vault",
            "internalConsoleOptions": "openOnSessionStart",
            "sourceMaps": true,
            "outputCapture": "std",
            "outFiles": [
                "${workspaceFolder}/Tasks/Vault/.bin/**/*.js"
            ]
        }
    ]
}

